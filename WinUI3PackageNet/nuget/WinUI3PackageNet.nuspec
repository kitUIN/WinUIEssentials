<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>WinUI3PackageNet</id>
    <version>0.1.14</version>
    <authors>Contoso Math Inc.</authors>
    <description>A simple component with basic math operations</description>
    <dependencies>
      <!--Note: Target .NET 5 by replacing instances of "net6.0" with "net5.0 in this file"-->
      <group targetFramework="net6.0-windows10.0.22621.0" />
    </dependencies>
  </metadata>
  <files>
	  <!-- .targets -->
	  <file src="..\WinUI3PackageNet.targets" target="build\"/>

	  <!--Note: This sample assumes the solution configuration and platform are set to Release and x64-->
    <file src="..\..\AnyCPU\Debug\WinUI3PackageNet\bin\WinUI3PackageNet.dll" target="lib\net6.0-windows10.0.22621.0\WinUI3PackageNet.dll" />
    <file src="..\..\AnyCPU\Debug\WinUI3PackageNet\bin\WinUI3PackageNet.pdb" target="lib\net6.0-windows10.0.22621.0\" />
    <file src="..\..\AnyCPU\Debug\WinUI3PackageNet\bin\WinUI3PackageNet.pri" target="lib\net6.0-windows10.0.22621.0\" />
    <file src="..\..\AnyCPU\Debug\WinUI3PackageNet\bin\WinUI3Package\*.*" target="lib\net6.0-windows10.0.22621.0\WinUI3Package\" />
    <!--Arch-specific implementation DLLs should be copied into RID-relative folders-->
    <file src="..\..\x64\Debug\WinUI3Package\bin\WinUI3Package\WinUI3Package.dll" target="runtimes\win10-x64\native" />
    <file src="..\..\x64\Debug\WinUI3Package\bin\WinUI3Package\WinUI3Package.pdb" target="runtimes\win10-x64\native" />
    <file src="..\..\x64\Debug\WinUI3Package\bin\WinUI3Package\WinUI3Package.pri" target="runtimes\win10-x64\native" />
    <!--To support x86 and ARM64, build SimpleMathComponent for those other architectures and uncomment the entries below.-->
    <!--<file src="..\..\_build\Win32\Release\SimpleMathComponent\bin\SimpleMathComponent\SimpleMathComponent.dll" target="runtimes\win10-x86\native\SimpleMathComponent.dll" />-->
    <!--<file src="..\..\_build\arm64\Release\SimpleMathComponent\bin\SimpleMathComponent\SimpleMathComponent.dll" target="runtimes\win10-arm64\native\SimpleMathComponent.dll" />-->
  </files>
</package>